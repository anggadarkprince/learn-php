{
    "name": "anggadarkprince/learn-unit-test",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Anggadarkprince\\LearnUnitTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anggadarkprince\\LearnUnitTest\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Angga Ari Wijaya",
            "email": "anggadarkprince@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "scripts": {
        "test" : "phpunit --configuration phpunit.xml",
        "test:product" : "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter" : "phpunit --configuration phpunit.xml --testsuite counter"
    }
}
